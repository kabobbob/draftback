<h2>Links</h2>
<p></p>
<div>
  <table width='100%' cellpadding='0' cellspacing='0'>
    <% i = 1%>
    <% for link in @links %>
    <% cell_class = alt %>
    <tr>
      <td <%= cell_class %>><%= i %>.</td>
      <td <%= cell_class %>><%= link.title %></td>
      <td <%= cell_class %>><%= link_to link.url, link.url, :target => '_blank' %></td>
    </tr>
    <tr>
      <td <%= cell_class %>></td>
      <td <%= cell_class %> colspan='2'><%= link.description %></td>
    </tr>
    <% i += 1 %>
    <% end %>
  </table>
</div>