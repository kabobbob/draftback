<h2>List Posts</h2>
<p></p>
<div>
  <table cellpadding="0" cellspacing="0">
    <tr>
      <th width="150px">Post</th>
      <th width="50px">By</th>
      <th width="155px">Created</th>
      <th width="70px">Displayed</th>
      <th></th>
    </tr>
    <%= render :partial => "post", :collection => @posts %>
    <tr>
      <td colspan="4" height="25px;">
        <%= link_to 'New Post', new_post_path %> | <%= link_to 'Back', manage_path() %>
      </td>
    </tr>
  </table>
</div>